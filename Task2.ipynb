import cv2
from matplotlib import pyplot as plt
path=r'C:\Users\sai\Desktop\PBL project\report\tree.jpg'
image=cv2.imread(path)
window_name='image'
cv2.imshow(window_name,image)
 
%matplotlib inline
img_rgb=cv2.cvtColor(image,cv2.COLOR_BGR2RGB)
